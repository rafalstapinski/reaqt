{
  "name": "reaqt",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json 'app/**/*.ts' 'app/**/*.tsx'",
    "start": "node ./dist/server/server.js",
    "start:prod": "cross-env NODE_ENV=production node ./dist/server/server.js",
    "build-client": "cd ./app/client && cross-env NODE_ENV=production webpack -p",
    "dev-client": "cd ./app/client && webpack-dev-server -w",
    "build-server": "tsc -p ./app/server",
    "dev-server": "tsc-watch -p ./app/server --onSuccess \"node ./dist/server/server.js\"",
    "dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\"",
    "gql": "cd ./scripts/generate-gql && tsc generate-gql.ts  --target \"es5\" --lib \"esnext\" --types \"node\" --moduleResolution \"node\" &&  node generate-gql.js ; tslint --fix app/typings/**.d.ts"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.1",
    "apollo-cache-inmemory": "^1.2.9",
    "apollo-client": "^2.4.1",
    "apollo-link-http": "^1.5.4",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "http-proxy-middleware": "^0.18.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "tslib": "^1.9.2"
  },
  "devDependencies": {
    "@gql2ts/from-query": "^1.8.2",
    "@gql2ts/from-schema": "^1.8.2",
    "@gql2ts/language-typescript": "^1.8.2",
    "@gql2ts/types": "^1.8.1",
    "@gql2ts/util": "^1.8.1",
    "@types/express": "^4.16.0",
    "@types/express-graphql": "^0.6.1",
    "@types/graphql": "^0.13.4",
    "@types/humps": "^1.1.2",
    "@types/node": "^10.3.3",
    "@types/react": "^16.4.12",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "apollo-codegen": "^0.20.2",
    "awesome-typescript-loader": "^5.0.0",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "gql2ts": "^1.8.2",
    "graphql": "^0.13.2",
    "graphql-cli": "^2.16.5",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "humps": "^2.0.1",
    "open-browser-webpack-plugin": "0.0.5",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "tsc-watch": "^1.0.22",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.6",
    "webpack-dev-server": "^3.1.4"
  }
}
